<body>
<figure class="cd-image-container">
  <%= image_tag 'web-1940.jpg', alt: 'Original Image' %>
  <span class="cd-image-label" data-type="original">1940-ві рр</span>

  <div class="cd-resize-img">
    <%= image_tag 'web-1780.jpg', alt: 'Modified Image', usemap: '#img-modified-jpg' %>
    <map id="img-modified" name="img-modified">
      <area data-key="place_1" shape="poly" alt="1" coords="100,100,100,200,200,200,200,100" nohref title="Обывательское строение" />
      <area shape="default" nohref="nohref" alt="" />
    </map>
    <span class="cd-image-label" data-type="modified">1782 р</span>
  </div>
  <span class="cd-handle"></span>
</figure>

<%= javascript_include_tag 'jquery.mobile.custom.min.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'mapshower.js', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'main.js', 'data-turbolinks-track': 'reload' %>
</body>
